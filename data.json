      "articles": {
          "add-team-members": {
              "id": "add-team-members",
              "title": "Add team members",
              "format": "md",
              "updatedAt": "2025-10-21",
              "body": "## Invite teammates\nGo to **Users \u2192 Invite**. Assign roles (Viewer, Editor, Admin, Superadmin) based on responsibilities.\n",
              "tags": [
                  "collaboration",
                  "users"
              ]
          },
          "api-access": {
              "id": "api-access",
              "title": "API access",
              "format": "md",
              "updatedAt": "2025-10-21",
              "body": "## API\nRequest an API key from **Settings \u2192 Developers**. Rate limits and scopes apply.\n",
              "tags": [
                  "api",
                  "developers"
              ]
          },
          "archiving-data": {
              "id": "archiving-data",
              "title": "Archiving old data",
              "format": "md",
              "updatedAt": "2025-10-21",
              "body": "## Archive policy\nArchive stale companies/investors/events to improve performance. Restore anytime.\n",
              "tags": [
                  "lifecycle",
                  "archive"
              ]
          },
          "ask-question-1": {
              "id": "ask-question-1",
              "title": "Ask a question or describe a (1)",
              "format": "md",
              "updatedAt": "2025-10-21",
              "body": "## Ask support\nTell us what you\u2019re trying to do and what you expected vs. what happened.\n",
              "tags": []
          },
          "ask-question-2": {
              "id": "ask-question-2",
              "title": "Ask a question or describe a (2)",
              "format": "md",
              "updatedAt": "2025-10-21",
              "body": "## Ask support v2\nInclude screenshots and the URL.\n",
              "tags": []
          },
          "ask-question-3": {
              "id": "ask-question-3",
              "title": "Ask a question or describe a (3)",
              "format": "md",
              "updatedAt": "2025-10-21",
              "body": "## Ask support v3\nWe\u2019ll follow up by email.\n",
              "tags": []
          },
          "bulk-import": {
              "id": "bulk-import",
              "title": "Bulk import data",
              "format": "md",
              "updatedAt": "2025-10-21",
              "body": "## Bulk import\nUse the **Bulk Import** tool for Companies, Investors, Events. Upload CSV, map columns, preview, then commit.\n",
              "tags": [
                  "import",
                  "csv"
              ]
          },
          "bulk-import-tools": {
              "id": "bulk-import-tools",
              "title": "Bulk import tools",
              "format": "md",
              "updatedAt": "2025-10-21",
              "body": "## Tools\nDownload sample CSVs, validate locally, then upload via **Bulk Import**.\n",
              "tags": [
                  "import",
                  "tools"
              ]
          },
          "companies-insights": {
              "id": "companies-insights",
              "title": "Companies \u2192 Insights",
              "format": "md",
              "updatedAt": "2025-10-21",
              "body": "## Company Insights\nFunding rounds, traction growth, hiring trends, and sector distribution.\nAdmins see advanced filters and cohort comparisons.\n",
              "tags": [
                  "companies",
                  "insights",
                  "metrics"
              ]
          },
          "company-registration": {
              "id": "company-registration",
              "title": "Company registration steps",
              "format": "md",
              "updatedAt": "2025-09-21",
              "body": "## Company registration\n1. Go to **Companies \u2192 Directory \u2192 Add company**.\n2. Provide legal name, sector, location, and contacts.\n3. Optionally attach pitch deck and logo.\n4. Submit for admin review (if moderation is enabled).\n",
              "tags": [
                  "companies",
                  "registration"
              ]
          },
          "content-studio-basics": {
              "id": "content-studio-basics",
              "title": "Content Studio \u2192 Basics1",
              "format": "md",
              "updatedAt": "2025-10-21",
              "body": "## Content Studio\nManage site copy, resources, feeds, message templates, presets, and analysis narratives.\n",
              "tags": [
                  "cms",
                  "content-studio"
              ]
          },
          "custom-templates": {
              "id": "custom-templates",
              "title": "Custom templates",
              "format": "md",
              "updatedAt": "2025-10-21",
              "body": "## Templates\nCreate reusable templates for reports, opportunities, and messages in **Content Studio**.\n",
              "tags": [
                  "templates"
              ]
          },
          "emergency-contacts": {
              "id": "emergency-contacts",
              "title": "Emergency contacts",
              "format": "md",
              "updatedAt": "2025-10-21",
              "body": "## Emergency contacts\nEmail: support@example.org\nPhone: +250 700 000 000\nSLA: P1 in 2h, P2 in 8h, P3 next business day.\n",
              "tags": [
                  "support",
                  "contact"
              ]
          },
          "enable-2fa": {
              "id": "enable-2fa",
              "title": "Enable 2FA",
              "format": "md",
              "updatedAt": "2025-09-19",
              "body": "## Two-factor authentication\nEnable 2FA in **Settings \u2192 Security**. Supported: TOTP apps (Authy, Google Authenticator). Recovery codes are shown once\u2014store securely.\n",
              "tags": [
                  "security",
                  "2fa",
                  "mfa"
              ]
          },
          "enablers-assessment": {
              "id": "enablers-assessment",
              "title": "Enablers \u2192 Assessment",
              "format": "md",
              "updatedAt": "2025-10-21",
              "body": "## ESO Assessment\nScorecards for hubs, incubators, universities. KPIs: programs, jobs, cohorts, follow-on capital.\n",
              "tags": [
                  "enablers",
                  "eso",
                  "assessment"
              ]
          },
          "export-reports": {
              "id": "export-reports",
              "title": "Export reports",
              "format": "md",
              "updatedAt": "2025-10-21",
              "body": "## Export\nFrom any analytics page, click **Export** to download CSV/XLSX or schedule emailed PDFs.\n",
              "tags": [
                  "reports",
                  "export"
              ]
          },
          "investors-directory": {
              "id": "investors-directory",
              "title": "Investors \u2192 Directory",
              "format": "md",
              "updatedAt": "2025-10-21",
              "body": "## Investor Directory\nBrowse funds, angels, and DFIs. Track theses, ticket sizes, and geo focus.\n",
              "tags": [
                  "investors",
                  "directory"
              ]
          },
          "keyboard-shortcuts": {
              "id": "keyboard-shortcuts",
              "title": "Keyboard shortcuts",
              "format": "md",
              "updatedAt": "2025-10-21",
              "body": "## Shortcuts\n- `/` focus search\n- `g c` go to Companies\n- `g i` go to Investors\n- `?` open help\n",
              "tags": [
                  "productivity",
                  "shortcuts"
              ]
          },
          "login-activity": {
              "id": "login-activity",
              "title": "Check login activity",
              "format": "md",
              "updatedAt": "2025-10-21",
              "body": "## Login activity\nReview recent sign-ins, IP addresses, and device info in **Settings \u2192 Security \u2192 Activity**. Click **Sign out of other devices** if needed.\n",
              "tags": [
                  "security",
                  "audit"
              ]
          },
          "login-devices": {
              "id": "login-devices",
              "title": "Managing login devices",
              "format": "md",
              "updatedAt": "2025-10-21",
              "body": "## Devices\nView and revoke remembered devices. Enable **device approval** for high-security organizations.\n",
              "tags": [
                  "security",
                  "devices"
              ]
          },
          "notifications": {
              "id": "notifications",
              "title": "Notification settings",
              "format": "md",
              "updatedAt": "2025-10-21",
              "body": "## Notifications\nEmail, in-app, and (optional) Slack notifications. Customize per module.\n",
              "tags": [
                  "notifications"
              ]
          },
          "opportunities-tenders": {
              "id": "opportunities-tenders",
              "title": "Opportunities \u2192 Tenders",
              "format": "md",
              "updatedAt": "2025-10-21",
              "body": "## Tenders\nPublish and manage RFPs/RFQs. Vendors submit bids; admins shortlist and award.\n",
              "tags": [
                  "opportunities",
                  "tenders",
                  "rfp"
              ]
          },
          "password-reset": {
              "id": "password-reset",
              "title": "Password reset guide",
              "format": "md",
              "updatedAt": "2025-09-19",
              "body": "## Reset your password\n1. Click **Forgot password** on the login screen.\n2. Enter your email and follow the link.\n3. Set a strong password (\u2265 12 chars, mixed types).\n\n> Admins can force a reset in **Users**.\n",
              "tags": [
                  "security",
                  "password"
              ]
          },
          "profile-setup": {
              "id": "profile-setup",
              "title": "Profile setup",
              "format": "md",
